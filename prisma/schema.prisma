// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "dart run orm"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id Int @id @default(autoincrement())
  fname String?
  lname String?
  email String?
  phone String?
  password String?
  profile String?
  role String?
  created_at DateTime?
  updated_at DateTime?
}

model tblPhoto{
  id Int @id @default(autoincrement())
  albumsid Int?
  title String?
  date DateTime?
  url String?
  path String?
  created_at DateTime?
  updated_at DateTime?
}

model tblalbums{
  id Int @id @default(autoincrement())
  idfolder String
  userid Int?
  title String?
  created_at DateTime?
  updated_at DateTime?
}

model tblhistory{
  id Int @id @default(autoincrement())
  userid Int?
  photoid Int?
  date DateTime?
  created_at DateTime?
  updated_at DateTime?
}


